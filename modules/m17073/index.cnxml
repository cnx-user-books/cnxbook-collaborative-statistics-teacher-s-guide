                      </m:mrow>
                    </m:mstyle>
                  </m:msub>
                  <m:mi>:</m:mi>
                  <m:mrow>
                    <m:msub>
                      <m:mi>σ</m:mi>
                      <m:mstyle fontsize="8pt">
                        <m:mrow>
