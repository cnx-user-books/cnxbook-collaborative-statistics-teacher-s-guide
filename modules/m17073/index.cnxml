                              </m:mrow>
                            </m:mstyle>
                          </m:msup>
                        </m:mrow>
                      </m:mstyle>
                    </m:msub>
                    <m:mo stretchy="false">&gt;</m:mo>
                    <m:msub>
                      <m:mi>σ</m:mi>
                      <m:msup>
                        <m:mi>B</m:mi>
                        <m:mstyle fontsize="6pt">
